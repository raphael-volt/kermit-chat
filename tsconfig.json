{
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "mat-avatars": [
        "dist/mat-avatars"
      ],
      "mat-avatars/*": [
        "dist/mat-avatars/*"
      ],
      "mat-rte": [
        "dist/mat-rte"
      ],
      "mat-rte/*": [
        "dist/mat-rte/*"
      ],
      "material": [
        "dist/material"
      ],
      "material/*": [
        "dist/material/*"
      ]
    },
    "sourceMap": true,
    "target": "es2015"
  }
}
