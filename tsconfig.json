{
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "paths": {
      "mat-avatars": [
        "projects/mat-avatars/src/public-api"
      ],
      "mat-rte": [
        "projects/mat-rte/src/public-api"
      ],
      "material": [
        "projects/material/src/public-api"
      ]
    },
    "sourceMap": true,
    "target": "es2015"
  }
}
